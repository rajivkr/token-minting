{
  "name": "mint-tokens-for-user",
  "version": "0.0.1",
  "private": true,
  "description": "",
  "license": "UNLICENSED",
  "author": "",
  "scripts": {
    "prebuild": "rimraf dist && yarn frontend:build",
    "build": "nest build",
    "dev:all": "concurrently \"yarn start:dev\" \"yarn frontend:dev\"",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "frontend:build": "cd frontend && yarn build",
    "frontend:dev": "cd frontend && yarn dev",
    "preinstall": "cd frontend && yarn",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prisma:generate": "prisma generate",
    "prisma:migrate-dev": "prisma migrate dev --name init",
    "prisma:migrate-reset": "prisma migrate reset",
    "prisma:seed-dev": "prisma db seed",
    "server:build": "nest build",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:dev": "env-cmd -f .env nest start --watch",
    "start:prod": "node dist/main",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:watch": "jest --watch"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testEnvironment": "node",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    }
  },
  "dependencies": {
    "@nestjs/common": "^9.3.9",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.3.9",
    "@nestjs/jwt": "^10.0.2",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.3.9",
    "@nestjs/serve-static": "^3.0.1",
    "@prisma/client": "4.11.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "env-cmd": "^10.1.0",
    "passport": "^0.6.0",
    "passport-headerapikey": "^1.2.2",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.2.0",
    "@nestjs/testing": "^9.3.9",
    "@types/node": "18.11.18",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "concurrently": "^7.6.0",
    "eslint": "^8.35.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "29.3.1",
    "prettier": "^2.8.4",
    "prisma": "^4.11.0",
    "supertest": "^6.3.3",
    "ts-jest": "29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
